<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tuan2.Views.FloytLayout.FlyoutPageDemo"
             xmlns:pages="clr-namespace:Tuan2.Views.FloytLayout"
             xmlns:view="clr-namespace:Tuan2">
    <FlyoutPage.Flyout>
        <pages:FlyoutPageDemoFlyout x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <view:MainPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>